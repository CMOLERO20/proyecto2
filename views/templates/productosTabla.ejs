<div>
<% if (productos.length) { %> 
    <h2>Lista de Productos</h2>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto</th>
            </tr>
            <% productos.forEach(producto => { %>  
            <tr>
                <td><%= producto.title %></td>
                <td><%= producto.price %></td>
                <td><img width="50" src= <%= producto.thumbnail %> alt="not found"></td>
            </tr>
            <% }) ; %>
        </table>
    </div>

<% } %>

</div>